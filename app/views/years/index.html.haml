%h1 Listing years
%table
  %thead
    %tr
      %th Year value
      %th User ID
      %th
      %th
      %th
  %tbody
    - @years.each do |year|
      %tr
        %td= year.year_value
        %td= year.user.id
        %td= link_to 'Show', year
        %td= link_to 'Edit', edit_year_path(year)
        %td= link_to 'Destroy', year, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Year', new_year_path
